{
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iis": {
      "applicationUrl": "http://localhost/DeviceService",
      "sslPort": 0
    },
    "iisExpress": {
      "applicationUrl": "http://localhost:58855",
      "sslPort": 44321
    }
  },
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "api/DeviceItems",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    },
    "DeviceService": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "api/DeviceItems",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "applicationUrl": "http://localhost:5000"
    },
    "Windows": {
      "commandName": "IIS"
    },
    "Docker": {
      "commandName": "Docker",
      "launchBrowser": true,
      "launchUrl": "{Scheme}://{ServiceHost}:{ServicePort}/api/DeviceItems",
      "publishAllPorts": true,
      "useSSL": true
    }
  }
}